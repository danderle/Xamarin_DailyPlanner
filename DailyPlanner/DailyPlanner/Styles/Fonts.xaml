<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">


    <system:String x:Key="FontAwesomeSolidFamily">FontAwesomeSolid</system:String>

    <system:Double x:Key="FontSizeSmall">20</system:Double>
    <system:Double x:Key="FontSizeMedium">30</system:Double>
    <system:Double x:Key="FontSizeLarge">40</system:Double>
    <system:Double x:Key="FontSizeXLarge">50</system:Double>

    <system:String x:Key="FontAwesomePlus">&#xf067;</system:String>
    <system:String x:Key="FontAwesomeAngleLeft">&#xf104;</system:String>
    <system:String x:Key="FontAwesomeAngleRight">&#xf105;</system:String>
    <system:String x:Key="FontAwesomeSave">&#xf0c7;</system:String>
    <system:String x:Key="FontAwesomeTrash">&#xf1f8;</system:String>
    <system:String x:Key="FontAwesomeEdit">&#xf044;</system:String>
    <system:String x:Key="FontAwesomeX">&#xf00d;</system:String>
    <system:String x:Key="FontAwesomeCheck">&#xf00c;</system:String>
    
</ResourceDictionary>